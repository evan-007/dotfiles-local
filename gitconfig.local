[core]
  editor = /usr/local/bin/nvim
  excludesfile = ~/.gitignore
[color]
  status = auto
  ff = auto
  branch = auto
[alias]
  co = checkout
  b = branch
  c = commit
  ds = diff --staged
  undo = reset HEAD~ # reverts last commit
  unstage = reset HEAD
  lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
